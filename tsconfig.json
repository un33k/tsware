{
  "version": "1.5.0",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "listFiles": true,
    "outDir": "dist",
    "experimentalDecorators": true,
    "declarationFiles": false,
    "noExternalResolve": true,
    "sortOutput": true,
    "preserveConstEnums": true      
  },
  "filesGlob": [
    "./utils/typings/references.d.ts"
  ]
}
