{
  "version": "1.5.0",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "listFiles": true,
    "outDir": "dist",
    "preserveConstEnums": true,
    "experimentalDecorators": true   
  },
  "filesGlob": [
    "toolbox/typings/tsd",
    "toolbox/typings/custom.d.ts",
    "toolbox/typings/app.d.ts"
  ]
}
